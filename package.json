{
  "name": "der-kleine-knick",
  "version": "0.0.1",
  "description": "Der kleine Knick",
  "author": "Philip Paetz",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": ">=5.9"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chapati23/der-kleine-knick.git"
  },
  "scripts": {
    "start": "npm run serve:dev",
    "serve:dev": "babel-node tools/dev-server",
    "serve:dist": "NODE_ENV=production babel-node tools/dist-server",
    "preserve:dist": "npm run build",
    "clean": "rimraf ./dist && mkdir dist",
    "lint": "eslint src/**/*.js webpack.config.babel.js",
    "prebuild": "npm run clean && babel-node tools/build-html",
    "build": "babel-node tools/build-webpack && echo www.derkleineknick.de > dist/CNAME",
    "deploy": "npm run build && ./tools/deploy.sh"
  },
  "dependencies": {
    "connect-history-api-fallback": "~1.2.0",
    "isomorphic-fetch": "~2.2.1",
    "normalize.css": "~4.1.1",
    "react": "~15.0.1",
    "react-css-modules": "~3.7.6",
    "react-dom": "~15.0.1",
    "react-multidecorator": "~0.1.0",
    "react-router": "~2.1.1"
  },
  "devDependencies": {
    "autoprefixer": "~6.3.6",
    "babel-cli": "~6.7.5",
    "babel-core": "~6.7.6",
    "babel-eslint": "~6.0.2",
    "babel-loader": "~6.2.4",
    "babel-plugin-react-transform": "~2.0.2",
    "babel-preset-es2015": "~6.6.0",
    "babel-preset-react": "~6.5.0",
    "babel-preset-stage-0": "~6.5.0",
    "browser-sync": "~2.12.2",
    "cheerio": "~0.20.0",
    "css-loader": "~0.23.1",
    "eslint": "~2.7.0",
    "eslint-loader": "~1.3.0",
    "eslint-plugin-react": "~4.3.0",
    "extract-text-webpack-plugin": "~1.0.1",
    "file-loader": "~0.8.5",
    "postcss-calc": "~5.2.1",
    "postcss-loader": "~0.8.2",
    "postcss-nested": "~1.0.0",
    "react-transform-catch-errors": "~1.0.2",
    "react-transform-hmr": "~1.0.4",
    "redbox-react": "~1.2.3",
    "rimraf": "~2.5.2",
    "static-site-generator-webpack-plugin": "~2.0.1",
    "style-loader": "~0.13.1",
    "webpack": "~1.12.15",
    "webpack-dev-middleware": "~1.6.1",
    "webpack-dev-server": "~1.14.1",
    "webpack-hot-middleware": "~2.10.0"
  }
}
